{
    "manifest_version":2,
    "name":"Protein Tracker",
    "description":"Tracks a user proteins total thorughout the day",
    "version":"0.1",
    "browser_action" : {
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "permissions" : [
        "storage",
        "notifications",
        "contextMenus"
    ],
    "options_page":"options.html",
    "background" : {
        "scripts": ["eventPage.js"],
        "persistent": false
    }
}